{
  "extends": "@gpn-prototypes/frontend-configs/tsconfig",
  "shouldExtractLiteralValuesFromEnum": true,
  "files": ["./src/types/assets.d.ts"],
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "**/dist/**/*"],
  "compilerOptions": {
    "jsx": "react",
    "sourceMap": true,
    "target": "ES2020",
    "noImplicitThis": false,
    "target": "ES2020",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
