    "bem-cn": "^3.0.1",
    "graphql": "^15.4.0",
    "lodash": "^4.17.20",
    "lodash.defaultto": "^4.14.0",
    "react": "^16.14.0",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-textarea-autosize": "^8.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "single-spa-react": "^3.1.0",
    "uniqid": "^5.2.0",
    "uuid": "^8.3.1"
